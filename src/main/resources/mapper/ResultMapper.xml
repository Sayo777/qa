<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.sayo.qa.dao.ResultMapper" >
  <resultMap id="BaseResultMap" type="com.sayo.qa.entity.Result" >
    <result column="task_id" property="taskId" jdbcType="INTEGER" />
    <result column="result_id" property="resultId" jdbcType="VARCHAR" />
    <result column="qa_process" property="qaProcess" jdbcType="VARCHAR" />
  </resultMap>
  <insert id="insert" parameterType="com.sayo.qa.entity.Result" >
    insert into result (task_id, result_id, qa_process
      )
    values (#{taskId,jdbcType=INTEGER}, #{resultId,jdbcType=VARCHAR}, #{qaProcess,jdbcType=VARCHAR}
      )
  </insert>
  <insert id="insertSelective" parameterType="com.sayo.qa.entity.Result" >
    insert into result
    <trim prefix="(" suffix=")" suffixOverrides="," >
      <if test="taskId != null" >
        task_id,
      </if>
      <if test="resultId != null" >
        result_id,
      </if>
      <if test="qaProcess != null" >
        qa_process,
      </if>
    </trim>
    <trim prefix="values (" suffix=")" suffixOverrides="," >
      <if test="taskId != null" >
        #{taskId,jdbcType=INTEGER},
      </if>
      <if test="resultId != null" >
        #{resultId,jdbcType=VARCHAR},
      </if>
      <if test="qaProcess != null" >
        #{qaProcess,jdbcType=VARCHAR},
      </if>
    </trim>
  </insert>

  <select id="selectResults" resultMap="BaseResultMap">
    select task_id, result_id, qa_process from result
  </select>
</mapper>